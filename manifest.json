 {
     "name": "work-timer",
     "description": "Timer to get shit done! Productive hour split 45/15",
     "version": "1.0",
     "manifest_version": 3,
     "action": {
         "default_icon": "timer.png",
         "default_popup": "popup.html"
     },
     "background": {
         "service_worker": "background.js"
     },
     "permissions": [
         "tabs"
     ]


 }